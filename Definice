def hello_print(n):
    print("Hello " * n)
def hello_str(n):
    return "Hello " * n
def max(x, y):
    if x > y:
        return x
    else:
        return y
def fact(n):
    if n < 2:
        return 1
    fact = 1
    for i in range(2, n + 1):
        fact = fact * i
    return fact
def amount(proc, total):
    return total / 100 * proc